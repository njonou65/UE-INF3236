        "import matplotlib.pyplot as plt\n",
        "import matplotlib.cm as cm\n",
        "\n",
        "p = Perceptron(weights=[0.1, 0.1],\n",
        "               learning_rate=0.3)\n",
        "number_of_colors = 7\n",
        "colors = cm.rainbow(np.linspace(0, 1, number_of_colors))\n",
        "\n",
