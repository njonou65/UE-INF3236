      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 391
        },
        "id": "IkeWpBkB0Nys",
        "outputId": "a4c79ea4-3af8-4db2-de66-51a91d7195fe"
