    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "Gm4fIcSqWNAo",
        "outputId": "8756d1e0-559b-427e-a15b-5c42efc7b7a6"
