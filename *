        "# alternative activation function\n",
        "def ReLU(x):\n",
        "    return np.maximum(0.0, x)\n",
        "\n",
        "# derivation of relu\n",
        "def ReLU_derivation(x):\n",
        "    if x <= 0:\n",
        "        return 0\n",
        "    else:\n",
        "        return 1"
      ]
    },
